{
  "name": "tripsha-app-api",
  "version": "2.0.0",
  "description": "A Node.js starter for the Serverless Framework with async/await and unit test support",
  "main": "handler.js",
  "package": {
    "individually": false
  },
  "scripts": {
    "offline": "sls offline start --debugOffline --noAuth --stage=staging",
    "deploy": "sls deploy --stage=staging",
    "remove": "sls remove --stage=staging",
    "deploy:staging": "sls deploy --stage=staging",
    "remove:staging": "sls remove --stage=staging",
    "deploy:prod": "sls deploy --stage=prod-api",
    "remove:prod": "sls remove --stage=prod-api",
    "fixtures": "sls fixtures",
    "doc-gen": "node ./api-doc-dist/doc-gen.js",
    "doc-deploy": "node ./api-doc-dist/doc-gen.js && aws s3 sync ./api-doc-dist/ s3://tripsha.api-doc",
    "doc-deploy-staging": "node ./api-doc-dist/doc-gen.js && aws s3 sync ./api-doc-dist/ s3://staging-apidoc",
    "test": "jest",
    "test:all": "jest --watchAll",
    "test:ci": "jest --ci --colors --coverage",
    "pretty": "prettier --write --tab-width 2 \"src/**/*.js\"",
    "create-domain": "sls create_domain",
    "delete-domain": "sls delete_domain"
  },
  "lint-staged": {
    "*.{js,json}": [
      "npm run pretty",
      "git add"
    ]
  },
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AnomalyInnovations/serverless-nodejs-starter.git"
  },
  "devDependencies": {
    "aws-sdk": "^2.824.0",
    "chai": "^4.2.0",
    "husky": "^1.3.1",
    "jest": "^24.9.0",
    "lint-staged": "^8.2.1",
    "prettier": "^1.19.1",
    "serverless-domain-manager": "^3.3.2",
    "serverless-hooks-plugin": "^1.1.0",
    "serverless-offline": "^3.25.6",
    "serverless-plugin-include-dependencies": "^4.1.0",
    "serverless-webpack": "^5.3.5",
    "serverless-websockets-plugin": "^1.0.0",
    "source-map-support": "^0.4.18",
    "webpack": "^4.45.0",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/runtime": "^7.12.5",
    "axios": "^0.21.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-source-map-support": "^1.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-runtime": "^6.26.0",
    "extendify": "^1.0.0",
    "fastest-validator": "^0.6.19",
    "glob": "^6.0.4",
    "lodash": "^4.17.20",
    "md5": "^2.3.0",
    "moment": "^2.29.1",
    "mongodb": "^3.6.3",
    "mongoose": "^5.11.11",
    "node-jose": "^2.0.0",
    "qs": "^6.9.4",
    "serverless-plugin-split-stacks": "^1.9.3",
    "stripe": "^8.130.0",
    "urldecode": "^1.0.1",
    "uuid": "^3.4.0",
    "yaml-js": "^0.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}

{
  "name": "tripsha-app-api",
  "version": "1.1.1",
  "description": "A Node.js starter for the Serverless Framework with async/await and unit test support",
  "main": "handler.js",
  "scripts": {
    "offline": "sls offline start --debugOffline --noAuth",
    "install-dynamodb": "sls dynamodb install",
    "deploy": "sls deploy",
    "remove": "sls remove",
    "fixtures": "sls fixtures",
    "doc-gen": "node ./api-doc-dist/doc-gen.js",
    "doc-deploy": "node ./api-doc-dist/doc-gen.js && aws s3 sync ./api-doc-dist/ s3://tripsha.api-doc",
    "doc-deploy-staging": "node ./api-doc-dist/doc-gen.js && aws s3 sync ./api-doc-dist/ s3://staging-apidoc",
    "test": "jest",
    "test:all": "jest --watchAll",
    "test:ci": "jest --ci --colors --coverage",
    "pretty": "prettier --write --tab-width 2 \"src/**/*.js\"",
    "create-domain": "sls create_domain",
    "delete-domain": "sls delete_domain"
  },
  "lint-staged": {
    "*.{js,json}": [
      "npm run pretty",
      "git add"
    ]
  },
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AnomalyInnovations/serverless-nodejs-starter.git"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "dynamodb": "^1.2.0",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.17.0",
    "serverless-domain-manager": "^3.2.1",
    "serverless-hooks-plugin": "^1.1.0",
    "serverless-offline": "^3.25.6",
    "serverless-webpack": "^5.3.0",
    "serverless-websockets-plugin": "^1.0.0",
    "source-map-support": "^0.4.18",
    "webpack": "^4.30.0",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/runtime": "^7.4.3",
    "aws-sdk": "^2.437.0",
    "axios": "^0.19.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.0-beta.6",
    "babel-plugin-source-map-support": "^1.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-runtime": "^6.26.0",
    "extendify": "^1.0.0",
    "fastest-validator": "^0.6.17",
    "glob": "^6.0.4",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mongodb": "^3.3.1",
    "mongoose": "^5.6.10",
    "node-jose": "^1.1.3",
    "qs": "^6.9.0",
    "serverless-plugin-split-stacks": "^1.9.1",
    "stripe": "^6.28.0",
    "urldecode": "^1.0.1",
    "uuid": "^3.3.2",
    "yaml-js": "^0.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}

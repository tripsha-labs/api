{
  "name": "tripsha-app-api",
  "version": "1.1.1",
  "description": "A Node.js starter for the Serverless Framework with async/await and unit test support",
  "main": "handler.js",
  "scripts": {
    "offline": "export AWS_REGION='us-east-1' && sls offline start --debugOffline --noAuth",
    "install-dynamodb": "sls dynamodb install",
    "deploy": "sls deploy",
    "remove": "sls remove",
    "doc-gen": "node ./api-doc-dist/doc-gen.js",
    "doc-deploy": "node ./api-doc-dist/doc-gen.js && aws s3 sync ./api-doc-dist/ s3://tripsha.api-doc",
    "test": "jest"
  },
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/AnomalyInnovations/serverless-nodejs-starter.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/runtime": "^7.4.3",
    "aws-sdk": "^2.437.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.0-beta.6",
    "babel-plugin-source-map-support": "^1.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "jest": "^24.7.1",
    "serverless-dynamodb-local": "^0.2.37",
    "serverless-offline": "^3.25.6",
    "serverless-webpack": "^5.2.0",
    "webpack": "^4.30.0",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "dynamodb": "^1.2.0",
    "extendify": "^1.0.0",
    "fastest-validator": "^0.6.17",
    "glob": "^6.0.4",
    "joi": "^14.3.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "serverless-reqvalidator-plugin": "^1.0.3",
    "source-map-support": "^0.4.18",
    "stripe": "^6.28.0",
    "uuid": "^3.3.2",
    "validate": "^4.5.1",
    "yaml-js": "^0.1.3"
  }
}
